<div class="container">
  <h1>New Game</h1>
  <%= @letters.join('   ') %>
  <form action="/score" method="post" data-turbo="false">
    <input type="text" name="guess" placeholder="Your guess">
    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <input type="submit">
  </form>
</div>
